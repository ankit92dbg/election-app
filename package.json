  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start  -- --reset-cache --port=8081",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "run:android": "npx react-native run-android ",
    "android:bundle": "npx react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle",
    "android:build": "cd android && ./gradlew assembleRelease && cd ..",
    "run:reactotron": "adb reverse tcp:9090 tcp:9090",
    "adb:reverse": "adb reverse tcp:8081 tcp:8081"
  },
